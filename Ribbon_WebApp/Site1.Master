                <asp:ImageButton ID="A12" runat="server" ImageUrl="images/icons/Suppliers-add.ico" Height="32" alt="Save" ToolTip="დამატება" OnClick="btn_AddSupplier_Click" />
            </div>
        </asp:Panel>

        <cc2:ModalPopupExtender ID="MPopup_Pnl_AddExpenses" PopupControlID="Pnl_AddExpenses" TargetControlID="btn_AddExpenses" CancelControlID="btn_Close_AddExpenses" runat="server" BackgroundCssClass="ModalBackground">

        </cc2:ModalPopupExtender>
        <asp:Panel ID="Pnl_AddExpenses" runat="server" CssClass="modalPopup" Height="500px">
            <div id="nm3" runat="server" style="max-height: 500px; overflow: auto;">
                <asp:ImageButton href="#" ID="btn_Close_AddExpenses" runat="server" data-rel="back" class="ui-btn ui-corner-all ui-shadow ui-btn-a ui-icon-delete ui-btn-icon-notext ui-btn-right" ToolTip="ფანჯრის დახურვა" />
                <fieldset>
                    <legend class="rsGridHeaderRow">შემოსავალი/გასავალი დამატება:</legend>
                    <table>
                        <tbody class="rsGridContainer">

                            <tr style="width: 99%;" class="rsGridDataRow">
                                <th class="auto-style5">
                                    <label for="ExpenseName" class="auto-style5">სახელწოდება:</label>
                                </th>

                                <td class="auto-style4">
                                    <input id="ExpenseName" runat="server" class="rsGridSelect" style="height:28px; width:410px" />
                                </td>

                                <th class="auto-style5">
                                    <label for="ExpensePrice" class="auto-style5">ღირებულება:</label>
                                </th>

                                <td class="auto-style4">
                                    <input id="ExpensePrice" runat="server" class="rsGridSelect" style="height:30px; width:410px" />
                                </td>
                            </tr>

                            <tr style="width: 99%;" class="rsGridDataRow">
                                <th class="auto-style5">
                                    <label for="ExpenseContractorId" class="auto-style5">კლიენტის სახ:</label>
                                </th>

                                <td class="auto-style4">
                                    <asp:DropDownList ID="ExpenseContractorId" runat="server" Height="32px" Width="410px" CssClass="rsGridSelect">
                                        <asp:ListItem Selected="True" Value="">აირჩიეთ კლიენტი</asp:ListItem>
                                    </asp:DropDownList>
                                    
                                </td>

                                <th class="auto-style5">
                                    <label for="ExpenseType" class="auto-style5">ხარჯის სახე:</label>
                                </th>

                                <td class="auto-style4">
                                    <asp:DropDownList ID="ExpenseType" runat="server" Height="32px" Cssclass="rsGridSelect" Width="410px">
                                        <asp:ListItem Selected="True" Value="0">აირჩიეთ ტიპი</asp:ListItem>
                                        <asp:ListItem Value="1">შემოსავალი</asp:ListItem>
                                        <asp:ListItem Value="-1">გასავალი</asp:ListItem>
                                    </asp:DropDownList>
                                </td>
                            </tr>

                            <tr style="width: 99%;" class="rsGridDataRow">
                                <th class="auto-style5"">
                                    <label for="ExpenseDate" class="auto-style5">გადახ. თარიღი:</label>
                                </th>
                                <td class="auto-style4">
                                    <cc2:CalendarExtender ID="CalendarExtender4" runat="server" Format="dd/MM/yyyy" FirstDayOfWeek="Monday" TargetControlID="ExpenseDate" />
                                    <asp:TextBox ID="ExpenseDate" runat="server" onclick="this.placeholder=''" onfocus="this.select()" onblur="this.placeholder=!this.placeholder?'ოპერაციის თარიღი':this.placeholder" placeholder="ოპერაციის თარიღი" CssClass="rsGridSelect" PaneWidth="120px" FirstDayOfWeek="Monday" Height="28px" Width="410px"></asp:TextBox>
                                </td>

                                <th class="auto-style5">
                                    <label for="ExpenseWay" class="auto-style5">გადახ. წყარო:</label>
                                </th>

                                <td class="auto-style4">
                                    <asp:DropDownList ID="ExpenseWay" runat="server" Height="32px" Width="410px" CssClass="rsGridSelect" >
                                        <asp:ListItem Selected="True" Value="0">აირჩიეთ მეთოდი</asp:ListItem>
                                        <asp:ListItem Value="1">ნაღდი / სალარო</asp:ListItem>
                                        <asp:ListItem Value="2">უნაღდო / ბანკი</asp:ListItem>
                                    </asp:DropDownList>
                                </td>
                            </tr>

                            <tr style="width: 99%;" class="rsGridDataRow">
                                <th class="auto-style5">
                                    <label for="ExpenseComment" class="auto-style5">დამატებითი ინფორმაცია</label>
                                </th>
                                <td colspan="3">
                                    <asp:TextBox ID="ExpenseComment" runat="server" TextMode="MultiLine" style="width: 91%; height:240px; text-align: left; float:left"  />
                                </td>
                            </tr>                          

                        </tbody>
                        <tfoot class="rsGridSummaryRow">
                            <tr>
                                <td colspan="4">
                                    <asp:Button ID="btn_add_expences" runat="server" CssClass="lnkSubmit1" Text="SAVE" OnClick="btn_add_expences_Click" />
                                    &nbsp;
                                    <asp:Button ID="btn_close_expences" runat="server" CssClass="lnkSubmit1" Text="Cancel" />
                                </td>
                            </tr>
                        </tfoot>
                    </table>
                </fieldset>
            </div>
        </asp:Panel>
        
        <cc2:ModalPopupExtender ID="MPopup_add_InJournal" PopupControlID="add_InJournal" TargetControlID="ImageButton3" CancelControlID="btn_Close_addInJournal" runat="server" BackgroundCssClass="ModalBackground">

        </cc2:ModalPopupExtender>
        <div id="add_InJournal" runat="server" class="modalPopup" >
            <asp:ImageButton href="#" ID="btn_Close_addInJournal" runat="server" data-rel="back" class="ui-btn ui-corner-all ui-shadow ui-btn-a ui-icon-delete ui-btn-icon-notext ui-btn-right" ToolTip="ფანჯრის დახურვა" />
            <asp:TextBox ID="txt_InJournal_Name" runat="server" placeholder='ვინ/სახელწოდება' onfocus="this.placeholder = ''" onblur="this.placeholder = 'მოკლე აღწერილობა'"  Style="border: solid 1.8px #7f7f7f; -moz-border-radius: 4px; -webkit-border-radius: 4px; width: 252px;"></asp:TextBox><br /><br />
            <asp:TextBox ID="txt_InJournal_Price" runat="server" placeholder='ვალის თანხა' onfocus="this.placeholder = ''" onblur="this.placeholder = 'მოკლე აღწერილობა'"  Style="border: solid 1.8px #7f7f7f; -moz-border-radius: 4px; -webkit-border-radius: 4px; width: 252px;"></asp:TextBox><br /><br />
            <asp:TextBox ID="txt_InJournal_Text" runat="server" TextMode="MultiLine" Rows="6" placeholder='მოკლე აღწერილობა' onfocus="this.placeholder = ''" onblur="this.placeholder = 'მოკლე აღწერილობა'"  Style="border: solid 1.8px #7f7f7f; -moz-border-radius: 4px; -webkit-border-radius: 4px; width: 250px;"></asp:TextBox><br /><br />
            <asp:Button ID="btn_add_InJournal" runat="server" Text="დამატება" Style="border: solid 1.8px #7f7f7f; -moz-border-radius: 4px; -webkit-border-radius: 4px; width: 120px; height: 30px" OnClick="btn_add_InJournal_Click" /> <br /><br />
        </div>

        <cc2:ModalPopupExtender ID="MPopup_Login" runat="server" PopupControlID="Pnl_Login" TargetControlID="ImgBtn_login" CancelControlID="Button2" BackgroundCssClass="ModalBackground">

        </cc2:ModalPopupExtender>
        <asp:Panel ID="Pnl_Login" runat="server" class="popup-wrapper" style="min-width: 400px;" DefaultButton="btn_login">
            <div class="popup-header">Welcome back </div>

            <div class="popup-content">
                <div class="form-group">
                    <asp:TextBox ID="username" runat="server" placeholder="Email / User Name" />
                </div>
                <div class="form-group">
                    <asp:TextBox ID="userpass" runat="server" TextMode="password" placeholder="Password" />
                </div>
                <div class="form-group">
                    <asp:Button ID="btn_login" runat="server" CssClass="btn-success" OnClick="btn_login_Click" style="width: 100%;" Text="Sign in" />
                    <asp:Button ID="Button2" runat="server" CssClass="lnkSubmit1" Text="Close" style="display: none;" />
                </div>
            </div>
        </asp:Panel> 

        <cc2:ModalPopupExtender ID="MPopup_error" runat="server" PopupControlID="ErrorBox" TargetControlID="Button100" CancelControlID="error_close" BackgroundCssClass="ModalBackground" >

        </cc2:ModalPopupExtender>        
        <div id="ErrorBox" runat="server" class="popup-wrapper" style="min-width: 400px;">
            <div class="popup-header">
                შ ე ც დ ო მ ა !
            </div>
            

            <div class="popup-content">
                
                <div class="form-group">
                    <h2>
                        <label style='background-image: url("https://cdn0.iconfinder.com/data/icons/shift-free/32/Error-24.png"); background-repeat: no-repeat;'></label>
                        <strong>
                            მომხმარებლის სახელი ან პაროლი არასწორია.
                        </strong>
                    </h2>
                </div>
                <div class="form-group">
                    <asp:Button ID="error_close" runat="server" CssClass="btn-danger" style="width: 100%;" Text="დახურვა"/>
                </div>
            </div>
        </div>
        <asp:Button ID="Button100" runat="server" Text="Button" Style="display: none;" />
            
        <asp:ContentPlaceHolder ID="Left_side" runat="server">
            
        </asp:ContentPlaceHolder>

        <asp:ContentPlaceHolder ID="Center_side" runat="server">   
            
        </asp:ContentPlaceHolder>
            
    </form>    
</body>
