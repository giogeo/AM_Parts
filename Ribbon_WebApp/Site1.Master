    <script type="text/javascript">
        $(document).ready(function () {
            document.getElementById('taxID').onchange = function () {
                var text = document.getElementById('taxID').value;
                var txt_l = text.length;
                // you can modify the validation sentence, for example, if text == "aaa", page will post back;

                if (txt_l > 8) {
                    __doPostBack("taxID", 0);
                }
                else {
                    return false;
                }
            }
        });
    </script> 

    <script type="text/javascript">
        tday = new Array("Sunday", "Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday");
        tmonth = new Array("01", "02", "03", "04", "05", "06", "07", "08", "09", "10", "11", "12");

        function GetClock() {
            var d = new Date();
            var nday = d.getDay(), nmonth = d.getMonth(), ndate = d.getDate(), nyear = d.getYear();
            if (nyear < 1000) nyear += 1900;
            var nhour = d.getHours(), nmin = d.getMinutes(), nsec = d.getSeconds(), ap;

            // if (nhour == 0) { ap = " AM"; nhour = 12; } 
            // else if (nhour < 12) { ap = " AM"; }
            // else if (nhour == 12) { ap = " PM"; }
            // else if (nhour > 12) { ap = " PM"; nhour -= 12; }

            if (nmin <= 9) nmin = "0" + nmin;
            if (nsec <= 9) nsec = "0" + nsec;
            document.getElementById('datebox').innerHTML = " " + ndate + "." + (nmonth + 1) + "." + nyear + " ";
            document.getElementById('clockbox').innerHTML = " " + nhour + ":" + nmin + ":" + nsec + "";
        }

        window.onload = function () {
            GetClock();
            setInterval(GetClock, 1000);
        }
    </script>  
    
    <script type="text/javascript">
        function clk_Del() {

            if (document.getElementById("CheckBox4").checked) {
                document.getElementById("CheckBox4").checked = false;
                __doPostBack("CheckBox4", 0);
            } else {
                document.getElementById("CheckBox4").checked = true;
                __doPostBack("CheckBox4", 0);
            }
        }

        function chked1() {

            if (document.getElementById('WeatherTbilisi').checked) {
                document.getElementById('WeatherBorjomi').checked = false;
                document.getElementById('WeatherBakuriani').checked = false;
                window.open("http://www.accuweather.com/en/ge/tbilisi/171705/daily-weather-forecast/171705", "_blank");
                // window.location.href = 'http://www.accuweather.com/en/ge/tbilisi/171705/daily-weather-forecast/171705';
            }
        }

        function chked2() {
            if (document.getElementById('WeatherBorjomi').checked) {
                document.getElementById('WeatherTbilisi').checked = false;
                document.getElementById('WeatherBakuriani').checked = false;
                window.open("http://www.accuweather.com/en/ge/borjomi/171480/weather-forecast/171480", "_blank");
                // window.location.href = 'http://www.accuweather.com/en/ge/borjomi/171480/weather-forecast/171480';
            }
        }

        function chked3() {
            if (document.getElementById('WeatherBakuriani').checked) {
                document.getElementById('WeatherTbilisi').checked = false;
                document.getElementById('WeatherBorjomi').checked = false;
                window.open("http://www.accuweather.com/en/ge/bakuriani/171485/weather-forecast/171485", "_blank");
            }
        }

        function InsDone() {
            alert('დამატება განხორციელდა წარმატებით');
            window.location = 'statements.aspx#statements';
